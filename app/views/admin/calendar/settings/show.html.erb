      <div class="flex border-t border-gray-100">
        <div class="flex w-full items-center justify-between p-6">
          <span class="flex flex-col">
            <% if @settings.google_calendar_name.present? %>
              <span class="text-sm font-medium text-gray-900">Connected: <%= content_tag :span, @settings.google_calendar_name, data: { reflex: "click->GoogleCalendar#reset_calendar" }, class: "cursor-pointer text-blue-500" %></span>
            <% else %>
              <span class="text-sm font-medium text-gray-900">Not Connected</span>
            <% end %>
            <% if @settings.google_calendar_access_token.present? &&  !@settings.google_calendar_id.present?  %>
              <%= content_tag :select, data: { reflex: "change->GoogleCalendar#update_calendar" }, class: "mt-1 block w-full pl-3 pr-10 py-2 text-base rounded-none border-b border-gray-100 focus:outline-none focus:ring-indigo-500 focus:border-indigo-500 sm:text-sm" do %>
                <option value="">Select Calendar</option>
                <% @calendars.items.each do |calendar| %>
                  <option value="<%= calendar.id %>:<%= calendar.summary %>"><%= calendar.summary %></option>
                <% end %>
              <% end %>
            <% end %>
          </span>
          <%= link_to redirect_url(subdomain: "", redirect_to: request.original_url) do %>
            <%= render ToggleComponent.new(title: "Google Calendar", enabled: @settings.google_calendar_access_token.present?) %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
